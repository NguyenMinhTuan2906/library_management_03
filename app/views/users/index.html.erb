<% provide :title, t(".alluser") %>
<h1><%= t ".alluser" %> </h1>

<%= will_paginate %>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: Settings.gravatar_user.size %>
      <%= link_to user.name, user %>
      (<%= user.email %>)
    </li>
  <% end %>
</ul>

<%= will_paginate %>
